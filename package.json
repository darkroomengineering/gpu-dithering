{
  "name": "satus",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "https": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@studio-freight/compono": "^0.0.20",
    "@studio-freight/hamo": "^0.6.18",
    "@storyblok/react": "^2.0.18",
    "@studio-freight/react-lenis": "0.0.21",
    "@studio-freight/tempus": "0.0.37",
    "@theatre/core": "^0.6.1",
    "@theatre/studio": "^0.6.1",
    "clsx": "^1.2.1",
    "gsap": "3.12.2",
    "idb-keyval": "^6.2.0",
    "jsonminify": "^0.4.2",
    "next": "13.4.7",
    "next-pwa": "5.6.0",
    "next-sitemap": "^4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "zustand": "4.3.8",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@next/bundle-analyzer": "^13.4.7",
    "@size-limit/preset-app": "^8.2.6",
    "@svgr/webpack": "^8.0.1",
    "critters": "^0.0.19",
    "cross-env": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "size-limit": "^8.2.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "react-is": "18.2.0"
    }
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/**/*.js"
    }
  ]
}
